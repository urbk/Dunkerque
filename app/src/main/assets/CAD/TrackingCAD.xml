<?xml version="1.0" encoding="UTF-8"?>
<TrackingData>
    <Sensors>
        <Sensor Type="EdgeBasedInitializationSensorSource">
            <SensorID>SandettieCAD</SensorID>
            <Parameters>
                <TriangleMesh>Sandettie/SurfaceModel.obj</TriangleMesh>
                <TrackingConfiguration>Sandettie/InitialPose.xml</TrackingConfiguration>
                <UseSensorsForPoseCreation>off</UseSensorsForPoseCreation>
                <InitializationTestMode>false</InitializationTestMode>
                <AutoInit>
                    <PresetType>outdoor</PresetType>
                </AutoInit>
                <EdgeAlignment>
                    <LineModel>Sandettie/LineModel.obj</LineModel>
                    <MinQuality>0.65</MinQuality>
                    <SearchRange>2500.0</SearchRange>
                    <NumFeatures>200</NumFeatures>
                </EdgeAlignment>
                <FeatureTracking>
                    <Parameters>
                        <MapExtensionMethod>narrow</MapExtensionMethod>
                        <NumExtensibleFeatures>3000</NumExtensibleFeatures>
                    </Parameters>
                    <FeatureOrientationAssignment>regular</FeatureOrientationAssignment>
                </FeatureTracking>
            </Parameters>
            <SensorCOS>
                <SensorCosID>TrackingPoseSandettie</SensorCosID>
            </SensorCOS>
            <SensorCOS>
                <SensorCosID>InitialPoseSandettie</SensorCosID>
            </SensorCOS>
            <SensorID>PorteCAD</SensorID>
            <Parameters>
                <TriangleMesh>Porte/SurfaceModel.obj</TriangleMesh>
                <TrackingConfiguration>Porte/InitialPose.xml</TrackingConfiguration>
                <UseSensorsForPoseCreation>off</UseSensorsForPoseCreation>
                <InitializationTestMode>false</InitializationTestMode>
                <AutoInit>
                    <PresetType>outdoor</PresetType>
                </AutoInit>
                <EdgeAlignment>
                    <LineModel>Porte/LineModel.obj</LineModel>
                    <MinQuality>0.55</MinQuality>
                    <SearchRange>2500.0</SearchRange>
                    <NumFeatures>350</NumFeatures>
                </EdgeAlignment>
                <FeatureTracking>
                    <Parameters>
                        <MapExtensionMethod>wide</MapExtensionMethod>
                        <NumExtensibleFeatures>4500</NumExtensibleFeatures>
                    </Parameters>
                    <FeatureOrientationAssignment>regular</FeatureOrientationAssignment>
                </FeatureTracking>
            </Parameters>
            <SensorCOS>
                <SensorCosID>TrackingPosePorte</SensorCosID>
            </SensorCOS>
            <SensorCOS>
                <SensorCosID>InitialPosePorte</SensorCosID>
            </SensorCOS>

            <SensorID>BoiteCAD</SensorID>
            <Parameters>
                <TriangleMesh>Boite/SurfaceModel.obj</TriangleMesh>
                <TrackingConfiguration>Boite/InitialPose.xml</TrackingConfiguration>
                <UseSensorsForPoseCreation>gravity</UseSensorsForPoseCreation>
                <InitializationTestMode>false</InitializationTestMode>
                <AutoInit>
                    <PresetType>off</PresetType>
                </AutoInit>
                <EdgeAlignment>
                    <LineModel>Boite/LineModel.obj</LineModel>
                    <MinQuality>0.55</MinQuality>
                    <SearchRange>3000.0</SearchRange>
                    <NumFeatures>80</NumFeatures>
                </EdgeAlignment>
                <FeatureTracking>
                    <Parameters>
                        <MapExtensionMethod>narrow</MapExtensionMethod>
                        <NumExtensibleFeatures>3000</NumExtensibleFeatures>
                    </Parameters>
                    <FeatureOrientationAssignment>regular</FeatureOrientationAssignment>
                </FeatureTracking>
            </Parameters>
            <SensorCOS>
                <SensorCosID>TrackingPoseBoite</SensorCosID>
            </SensorCOS>
            <SensorCOS>
                <SensorCosID>InitialPoseBoite</SensorCosID>
            </SensorCOS>
        </Sensor>
    </Sensors>


    <Connections>
        <!--COS Sandettie-->
        <COS>
            <Name>TrackingPoseSandettie</Name>
            <SensorSource>
                <SensorID>SandettieCAD</SensorID>
                <SensorCosID>TrackingPoseSandettie</SensorCosID>
            </SensorSource>
        </COS>
        <COS>
            <Name>InitialPoseSandettie</Name>
            <SensorSource>
                <SensorID>SandettieCAD</SensorID>
                <SensorCosID>InitialPoseSandettie</SensorCosID>
            </SensorSource>
        </COS>
        <!--COS Porte-->
        <COS>
            <Name>TrackingPosePorte</Name>
            <SensorSource>
                <SensorID>PorteCAD</SensorID>
                <SensorCosID>TrackingPosePorte</SensorCosID>
            </SensorSource>
        </COS>
        <COS>
            <Name>InitialPosePorte</Name>
            <SensorSource>
                <SensorID>PorteCAD</SensorID>
                <SensorCosID>InitialPosePorte</SensorCosID>
            </SensorSource>
        </COS>
        <!--COS BOITE-->
        <COS>
            <Name>TrackingPoseBoite</Name>
            <SensorSource>
                <SensorID>BoiteCAD</SensorID>
                <SensorCosID>TrackingPoseBoite</SensorCosID>
            </SensorSource>
        </COS>
        <COS>
            <Name>InitialPoseBoite</Name>
            <SensorSource>
                <SensorID>BoiteCAD</SensorID>
                <SensorCosID>InitialPoseBoite</SensorCosID>
            </SensorSource>
        </COS>
    </Connections>
</TrackingData>
